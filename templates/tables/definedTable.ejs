<table class="overviewSummary" border="0" cellpadding="3" cellspacing="0">
  <tr>
    <th class="colFirst" scope="col">Properties Defined</th>
    <th class="colMiddle">Constraints</th>
    <th class="colLast" scope="col">Value</th>
  </tr>
  <% var index = 0 %>
  <% if ('pValue' in element) { -%>
    <% element.pValue.forEach(function(value) { %>
      <%- include('fieldRow', { field: value, index: index, dataElements: dataElements }) -%>
    <% index += 1; }) %>
  <% } -%>
  <% if (element.fields) { -%>
    <% element.fields.forEach(function(field) { -%>
      <% if (!('inheritance' in field) && (field.path == 'qdm-attribute' || field.path == 'ecqm-dataelement') ) { -%>
        <% if (field.card) { %>
          <% if (field.card.max != 0 ) { %>
            <%- include('fieldRow', { field: field, index: index, dataElements: dataElements }) -%>
          <% } %>
        <% } else { %>
          <%- include('fieldRow', { field: field, index: index, dataElements: dataElements }) -%>
        <% } %>
      <% index += 1; } -%>
    <% }) -%>
    <% var index = 0 %>
    <% element.fields.forEach(function(field) { -%>
      <% if (('inheritance' in field) && (field.path == 'qdm-attribute' || field.path == 'ecqm-dataelement') ) { -%>
        <% if (field.card) { %>
          <% if (field.card.max != 0 ) { %>
            <%- include('fieldRow', { field: field, index: index, dataElements: dataElements }) -%>
          <% } %>
        <% } else { %>
          <%- include('fieldRow', { field: field, index: index, dataElements: dataElements }) -%>
        <% } %>
      <% index += 1; } -%>
    <% }) -%>


    <!-- <% element.fields.forEach(function(field) { -%>
      <% if (!('inheritance' in field) && field.path != 'qdm-attribute') { -%>
        <%- include('fieldRow', { field: field, index: index }) -%>
      <% index += 1; } -%>
    <% }) -%>
    <% var index = 0 %>
    <% element.fields.forEach(function(field) { -%>
      <% if (('inheritance' in field) && field.path != 'qdm-attribute') { -%>
        <%- include('fieldRow', { field: field, index: index }) -%>
      <% index += 1; } -%>
    <% }) -%> -->
  <% } -%>
</table>